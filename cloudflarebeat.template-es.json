{
  "index_patterns": [
    "cloudflarebeat-6.2.4-*"
  ],
  "mappings": {
    "doc": {
      "_meta": {
        "version": "6.2.4"
      },
      "date_detection": false,
      "dynamic_templates": [
        {
          "fields": {
            "mapping": {
              "type": "keyword"
            },
            "match_mapping_type": "string",
            "path_match": "fields.*"
          }
        },
        {
          "strings_as_keyword": {
            "mapping": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "match_mapping_type": "string"
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "CacheCacheStatus": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "CacheResponseBytes": {
          "type": "long"
        },
        "CacheResponseStatus": {
          "type": "short"
        },
        "CacheTieredFill": {
          "type": "boolean"
        },
        "ClientASN": {
          "type": "long"
        },
        "ClientCountry": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "ClientDeviceType": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "ClientIP": {
          "type": "ip"
        },
        "ClientIPClass": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "ClientRequestBytes": {
          "type": "long"
        },
        "ClientRequestHost": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "ClientRequestMethod": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "ClientRequestProtocol": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "ClientRequestReferer": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "ClientRequestURI": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "ClientRequestUserAgent": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "ClientSSLCipher": {
          "norms": false,
          "type": "text"
        },
        "ClientSSLProtocol": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "ClientSrcPort": {
          "type": "long"
        },
        "EdgeColoID": {
          "type": "short"
        },
        "EdgeEndTimestamp": {
          "type": "date"
        },
        "EdgePathingOp": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "EdgePathingSrc": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "EdgePathingStatus": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "EdgeRequestHost": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "EdgeResponseBytes": {
          "type": "long"
        },
        "EdgeResponseCompressionRatio": {
          "type": "long"
        },
        "EdgeResponseContentType": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "EdgeResponseStatus": {
          "type": "short"
        },
        "EdgeServerIP": {
          "type": "ip"
        },
        "EdgeStartTimestamp": {
          "type": "date"
        },
        "OriginIP": {
          "type": "ip"
        },
        "OriginResponseBytes": {
          "type": "long"
        },
        "OriginResponseHTTPExpires": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "OriginResponseHTTPLastModified": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "OriginResponseStatus": {
          "type": "short"
        },
        "OriginResponseTime": {
          "type": "long"
        },
        "OriginSSLProtocol": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "RayID": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "SecurityLevel": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "WAFAction": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "WAFFlags": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "WAFMatchedVar": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "WAFProfile": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "WAFRuleID": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "WAFRuleMessage": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "Zone": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "ZoneID": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "beat": {
          "properties": {
            "hostname": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "name": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "timezone": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "version": {
              "ignore_above": 1024,
              "type": "keyword"
            }
          }
        },
        "error": {
          "properties": {
            "code": {
              "type": "long"
            },
            "message": {
              "norms": false,
              "type": "text"
            },
            "type": {
              "ignore_above": 1024,
              "type": "keyword"
            }
          }
        },
        "fields": {
          "type": "object"
        },
        "tags": {
          "ignore_above": 1024,
          "type": "keyword"
        }
      }
    }
  },
  "order": 1,
  "settings": {
    "index": {
      "mapping": {
        "total_fields": {
          "limit": 10000
        }
      },
      "number_of_routing_shards": 30,
      "refresh_interval": "5s"
    }
  }
}